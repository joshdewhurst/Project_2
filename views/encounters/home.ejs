<h2>All Encounters</h2>

<a href="./encounters/new">New Encounter</a>

<% console.log(encounters) %>

<% if(user) { %>
  <% encounters.forEach(encounter => { %>
      <div>
        <h2>
          <a href="/encounters/<%= encounter.id %>"><%= encounter.date %></a>
          <p><%= encounter.note %></p>
        </h2>
        <a href="/encounters/edit/<%= encounter.id %>"> Edit </a>
      </div>
    <% })}; %>