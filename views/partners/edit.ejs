<form method="POST", action='/partners/<%= id %>/?_method=PUT'>
    <label>Name</label>
    <input type="text" name="name" value="<%= partner.name %>" required>
    <label>Notes</label>
    <input type="text" name="note" value="<%= partner.note %>" placeholder="Add a note to this partner">
    <input type="submit">
</form>