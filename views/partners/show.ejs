<div class="profile">
    <h1><%= partner.name %></h1>

    <div id="show_encounter">

        <h2>Notes</h2>
        <p>
         <%= partner.note %>
        </p>
        <h3>
            How did you meet them?
        </h3>
        <p>
            <%= partner.met %>
        </p>
        <div>
            <form method="POST" action ="/partners/<%=partner.id%>/?_method=DELETE">
                <input type="submit" value="delete">
            </form>
            <button type="button" class="btn btn-outline-dark link-dark">
                <a class="link-dark" href="/partners/edit/<%= partner.id %>"> Edit </a>
            </button>
        </div>
    </div>
    <a class="link-dark" href="/partners">Back to All Partners</a>
</div>